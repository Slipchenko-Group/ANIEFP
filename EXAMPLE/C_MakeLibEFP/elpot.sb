#!/bin/bash

# NAME OF THE QUEUE IN USE
#SBATCH -A standby

# AMOUNT OF NODES AND PROCESSORS PER NODE REQUESTED
#SBATCH --nodes=1
#SBATCH -n 20
#SBATCH --exclusive
#SBATCH --mem=6400

# AMOUNT OF RESERVED TIME IN THE COMMUNITY CLUSTER. ONE WEEK = 168 HOURS
#SBATCH -t 4:00:00

# CHANGE TO THE DIRECTORY FROM WHICH YOU ORIGINALLY SUBMITTED THIS JOB
cd $SLURM_SUBMIT_DIR
for i in *.in
do
    /scratch/halstead/c/cviquezr/libefp-master/efpmd/src/efpmd $i > $i.out
done
